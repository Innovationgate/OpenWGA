<div class="module" style="<tml:option name="style"/>">
	<tml:script>
		_config = CMM.getModuleConfig($option_mod.module_id)
	</tml:script>
	<a class="title" data-id="<tml:item name="$option_mod.id"/>">
		<tml:script expression="String($option_mod.title)"/>
		<tml:option name="title"/>
		<tml:case isempty="_config">- Inhaltsmodul nicht gefunden</tml:case>
	</a>
	<tml:option name="body"/>
</div>