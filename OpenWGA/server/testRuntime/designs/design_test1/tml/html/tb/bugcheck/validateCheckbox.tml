<html> 
	<head> 
		<tml:htmlhead/>
	</head> 
	<body>
		<tml:action id="validate">
			this.tmlform.validate();
		</tml:action>
		
		<tml:form id="sonstwas" editable="true">			
			<tml:input name="bla" type="text"/><br>
			<%--<tml:input name="check" type="checkbox" options="Hallo,Welt" validation="$E_VALUE.size()!=0" message="Darf nicht leer sein."/>--%>
			<tml:input name="radio" type="radio" options="Hallo,Welt" validation="$E_VALUE=='Hallo'" message="Wählen Sie Hallo."/><BR>
			<tml:input name="boolean" type="boolean" options="Ja gerne! |true, Nein danke! |false" validation="$E_VALUE=='true'" message="Wählen Sie Ja."/>
			<tml:button clickaction="validate">validate</tml:button>
			<tml:formmessages/>
		</tml:form>

