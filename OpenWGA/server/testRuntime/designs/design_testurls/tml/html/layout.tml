<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<tml:htmlhead/>
	</head>

	<body>
		
		<tml:case isroot="false">
			Up to <tml:link context="parent"/>
		</tml:case>
		
		<ul>
			<tml:siblings>
				<li><tml:link/>
				<tml:case isselected="true">
					<ul>
						<tml:children>
							<li><tml:link/>
						</tml:children>
					</ul>
				</tml:case>
			</tml:siblings>
		</ul>
	
		<p id="pageid"><tml:meta name="key"/></p>
		<tml:innerlayout/>
	</body>
</html>