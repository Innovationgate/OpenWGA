<tml:form id="newdoc" contentclass="{newdocclass}" source="none">
	<div id="buttons">
		<tml:button id="store" clickaction="$store" portletmode="browse" portletcontext="createddoc" ajax="false">Save</tml:button>
		<tml:button id="cancel" clickaction="$refresh" portletmode="browse" confirm="Are you sure you want to cancel editing?" ajax="false">Cancel</tml:button>
	</div>
	
	<tml:include ref="mod:form-messages"/>
	<tml:include ref="[overlay:forms:{newdocclass}]"/>
</tml:form>

